{
    "name": "rebelcode/rcmod-eddbk-booking-logic",
    "description": "A RebelCode module for the booking logic in EDD Bookings.",
    "type": "rc-module",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "RebelCode",
            "email": "dev@rebelcode.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/RebelCode/rcmod-booking-logic"
        }
    ],
    "require": {
        "php": "^5.4 | ^7.0",
        "rebelcode/modular": "dev-task/initial-classes",
        "rebelcode/rcmod-booking-logic": "dev-task/initial-module-code",
        "rebelcode/wp-cqrs-resource-models": "dev-task/initial-classes",
        "rebelcode/sql-expression-renderer": "dev-task/initial-classes"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "^0.1",
        "codeclimate/php-test-reporter": "<=0.3.2"
    },
    "autoload": {
        "psr-4": {
            "RebelCode\\EddBookings\\Logic\\Module\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    }
}
